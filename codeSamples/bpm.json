            "bypass": false,
            "onCreate": "locals.timeoutTime = 0",
            "onValue": "if(get('this')=='beat')\n{\n\n  set('beat',get('beat')+1)\n}\nelse\n{\n\n  var bpm = get('this');\n  var bps = bpm/60.0\n  var beatDuration = 1/bps\n  \n  console.log('update'+get('this')+' '+beatDuration)\n  locals.timeoutTime = 1000*beatDuration\n}\n\nsetTimeout(function(){\n  set('this','beat')\n  \n},locals.timeoutTime)",
